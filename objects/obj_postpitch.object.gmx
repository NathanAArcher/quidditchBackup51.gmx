            <kind>1</kind>
            <string>draw_set_alpha(1)
draw_set_font(fnt_pregame)
draw_set_halign(fa_center)

//if scores are equal, tie = 1


//scheduleList[0] = first game
//scheduleList[7] = 8th game; 4 wins + 4 losses will access scheduleList[7], the 8th game.
if global.losses+global.wins+tie &lt;= 8{ //grabs the other team for the first 8 games
    if(global.losses+global.wins-1+tie &lt; 0){
        global.team2_name = "Out of bounds; L:" + string(global.losses) + " W:" + string(global.wins) + " T:" + string(tie)
        global.team2_color_secondary = c_white
        global.team2_color = c_black
        global.team2_logo = 1     
    }else{
        scr_random_name(global.scheduleList[global.losses+global.wins-1+tie])
    }
}

//first game of pool play is the 9th game played.
//5 wins + 4 losses = global.poolPlay[0]
if (global.losses+global.wins+tie &gt; 8) and (global.wins + global.losses+tie &lt;= 12){ //grabs the other team for the 4 regionals pool play games
scr_random_name(global.poolPlay[global.losses+global.wins-8-1+tie]) //-1 deleted
}

//9 wins + 4 losses = global.bracketList
//10 wins + 4 losses = sweet sixteen
//11 wins + 4 losses = elite 8
//12 wins + 4 losses = final 4
//13 wins + 4 losses = top 2
if (global.losses+global.wins+tie &gt; 12) and (global.wins+global.losses+tie+global.bracketGamesMissed &lt;= 17){
    //DISREGARD TIE. Go by wins and losses ONLY
    if global.losses+global.wins == 13{
        scr_random_name(global.bracketList[31-global.seed])
    }
    if global.losses+global.wins == 14{
        scr_random_name(global.sweetSixteenList[ 15-global.seed])
    }
    if global.losses+global.wins == 15{
        scr_random_name(global.eliteEightList[ 7-global.seed])
    }
    if global.losses+global.wins == 16{
        scr_random_name(global.finalFourList[ 3-global.seed])
    }
    if global.losses+global.wins == 17{
        scr_random_name(global.topTwoList[ 1-global.seed])
    }
}

if (global.losses+global.wins+tie+global.bracketGamesMissed &gt; 17) and (global.wins + global.losses+tie+global.bracketGamesMissed &lt;= 22){
    if global.losses+global.wins+global.bracketGamesMissed == 18{
        scr_random_name(global.bracketList[ 31-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 19{
        scr_random_name(global.sweetSixteenList[ 15-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 20{
        scr_random_name(global.eliteEightList[ 7-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 21{
        scr_random_name(global.finalFourList[ 3-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 22{
        scr_random_name(global.topTwoList[ 1-global.seed])
    }
}
if room == rm_postpitch{
    draw_text_with_outline(316, 92, string(global.team1_name), fnt_pregame, global.team1_color, c_black)
    if global.snitchgrab == 1{
        draw_text_with_outline(316, 128, string(global.team1_score)+ "*", fnt_pregame, global.team1_color, c_black)
    }else{
        draw_text_with_outline(316, 128, string(global.team1_score), fnt_pregame, global.team1_color, c_black)
    }
    if global.team1_score != global.team2_score{
        draw_text_with_outline(316, 256, "Final Score", fnt_pregame, c_white, c_black)
    }else{
        if global.overtime == 1{
        draw_text_with_outline(316, 256, "OVERTIME", fnt_pregame, c_white, c_black)
        }else{
        draw_text_with_outline(316, 256, "DOUBLE OVERTIME", fnt_pregame, c_white, c_black)
        }
    }
    if string_length(global.team2_name) &lt; 28{
        draw_text_with_outline(316, 412, string(global.team2_name), fnt_pregame, global.team2_color, c_black)
    }else{
        draw_text_transformed_with_outline(316, 412, string(global.team2_name), fnt_pregame, global.team2_color, c_black, .66, 1)
    }
    if global.snitchgrab == 2{
        draw_text_with_outline(316, 448, string(global.team2_score)+ "*", fnt_pregame, global.team2_color, c_black)
    }else{
        draw_text_with_outline(316, 448, string(global.team2_score), fnt_pregame, global.team2_color, c_black)
    }
}else{
    draw_text_with_outline(view_xview[0]+316, view_yview[0]+92, string(global.team1_name), fnt_pregame, global.team1_color, c_black)
    if global.snitchgrab == 1{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+128, string(global.team1_score)+ "*", fnt_pregame, global.team1_color, c_black)
    }else{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+128, string(global.team1_score), fnt_pregame, global.team1_color, c_black)
    }
    if global.team1_score != global.team2_score{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+256-64, "Final Score", fnt_pregame, c_white, c_black)
    }else{
        if global.overtime == 1{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+256-64, "OVERTIME", fnt_pregame, c_white, c_black)
        }else{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+256-64, "DOUBLE OVERTIME", fnt_pregame, c_white, c_black)
        }
    }
    if string_length(global.team2_name) &lt; 28{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+412-128, string(global.team2_name), fnt_pregame, global.team2_color, c_black)
    }else{
        draw_text_transformed_with_outline(view_xview[0]+316, view_yview[0]+412-128, string(global.team2_name), fnt_pregame, global.team2_color, c_black, .66, 1)
    }
    if global.snitchgrab == 2{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+448-128, string(global.team2_score)+ "*", fnt_pregame, global.team2_color, c_black)
    }else{
        draw_text_with_outline(view_xview[0]+316, view_yview[0]+448-128, string(global.team2_score), fnt_pregame, global.team2_color, c_black)
    }
}
