//first 8 games are on the schedule ([0]-&gt;[7])
if global.losses+global.wins &lt; 8{
scr_random_name(global.scheduleList[global.losses+global.wins])
}

//games 9, 10, 11, 12 [8]-&gt;[11]are pool play. Game 12 will have 11 (global.wins + global.losses)
if (global.losses+global.wins &gt;= 8) and (global.wins + global.losses &lt;= 11){ //grabs the other team for the 4 regionals pool play games
scr_random_name(global.poolPlay[global.losses+global.wins-8]) //-1 deleted
}

//games 13-&gt;17 ([12]-&gt;[16]) are pool play
//12 global.wins + losses for first bracket game
//13 global.wins + losses for sweet 16
//14 global.wins + losses for elite 8
//15 global.wins + losses for final 4
//16 global.wins + losses for finals
if (global.losses+global.wins+global.bracketGamesMissed &gt; 11) and (global.wins+global.losses+global.bracketGamesMissed &lt;= 16){
    if global.losses+global.wins+global.bracketGamesMissed == 12{ //beginning of first bracket play game
        scr_random_name(global.bracketList[ 31-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 13{ //beginning of second bracket play game
        if(15-global.seed) &lt; 0{
            show_message("INCOMING ERROR; Current seed is " + string(global.seed))
        }
        scr_random_name(global.sweetSixteenList[ 15-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 14{
        scr_random_name(global.eliteEightList[ 7-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 15{
        scr_random_name(global.finalFourList[ 3-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 16{
        scr_random_name(global.topTwoList[ 1-global.seed])
    }
}

//[17] is the first game of worlds brackets, regardless of previous performance!
if (global.losses+global.wins+global.bracketGamesMissed &gt; 16) and (global.wins + global.losses+global.bracketGamesMissed &lt;= 21){
    if global.losses+global.wins+global.bracketGamesMissed == 17{
        scr_random_name(global.bracketList[ 31-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 18{
        scr_random_name(global.sweetSixteenList[ 15-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 19{
        scr_random_name(global.eliteEightList[ 7-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 20{
        scr_random_name(global.finalFourList[ 3-global.seed])
    }
    if global.losses+global.wins+global.bracketGamesMissed == 21{
        scr_random_name(global.topTwoList[ 1-global.seed])
    }
}

