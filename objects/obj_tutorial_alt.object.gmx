                if global.threedmode == false{
                    vspeed -= (obj_player_test.st_power*.1)
                }else{
                    vspeed -= (obj_player_test.st_power*.1)
                }
            }
        }
        //WASD AND ZQSD use D and S the same way
        if(keyboard_check_direct(ord("D"))){ //RIGHT
                if global.threedmode == false{
